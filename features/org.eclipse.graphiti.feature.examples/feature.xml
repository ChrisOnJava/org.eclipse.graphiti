<feature
      id="org.eclipse.graphiti.feature.examples"
      label="%featureName"
      version="0.7.0.qualifier"
      provider-name="%providerName">

   <description>
      Graphiti supports the fast and easy creation of unified graphical
tools, which can graphically display and edit underlying domain
models using a tool-defined notation. These are some examples how to build tools on top of Graphiti.
   </description>

   <copyright url="http://www.eclipse.org/legal/epl-v10.html">
      Copyright (c) 2005, 2010 SAP AG.
All rights reserved.   This program and the accompanying materials
are made available under the terms of the Eclipse Public License
v1.0
which accompanies this distribution, and is available at
http://www.eclipse.org/legal/epl-v10.html
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import plugin="org.eclipse.help"/>
      <import plugin="org.eclipse.graphiti" version="0.7.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.graphiti.ui" version="0.7.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.graphiti.pattern" version="0.7.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.navigator"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.gef" version="3.5.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.views.properties.tabbed" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf.transaction" version="1.3.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import feature="org.eclipse.graphiti.feature" version="0.7.0.qualifier"/>
   </requires>

   <plugin
         id="org.eclipse.graphiti.doc"
         download-size="0"
         install-size="0"
         version="0.7.0.qualifier"
         unpack="false"/>

   <plugin
         id="org.eclipse.graphiti.examples.common"
         download-size="0"
         install-size="0"
         version="0.7.0.qualifier"
         unpack="false"/>

   <plugin
         id="org.eclipse.graphiti.examples.tutorial"
         download-size="0"
         install-size="0"
         version="0.7.0.qualifier"
         unpack="false"/>

</feature>
